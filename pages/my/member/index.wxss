  color: #fff;
}
.main .content .two .money i{
  font-size: 48rpx;
  font-weight: bold;
}
.main .content .two .delmoney{
  text-decoration: line-through;
}
.main .content .two .wei{
  font-size: 24rpx;
}
.main .content .btn{
  width: 590rpx;
  height: 80rpx;
