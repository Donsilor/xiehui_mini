  border-radius: 12rpx 0rpx 12rpx 0rpx;
  color: #fff;
}
.selected{background-color:#eacd98}
.main .content .one .money i{
  font-size: 48rpx;
  font-weight: bold;
}
.main .content .one .delmoney{
