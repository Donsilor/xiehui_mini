  font-size: 24rpx;
  text-align: center;
  color: #808080;
  display: flex;
  justify-content: space-around;
}
.main .zhuan .category .icon .right1{
  width: 68rpx;
  height: 29rpx;
  float: right;
}
.main .zhuan .category .icon .right{
  background-color: #fe5b11;
  width: 68rpx;
  height: 29rpx;
  font-size: 18rpx;
  color: #fff;
  border-radius: 14.5rpx;
  text-align: center;
