  z-index: -1;
}
.main .zhuan .category .icon i{
  display: flex;
  justify-content: center;    
}
.main .zhuan .category .icon i image{
  width: 79rpx;
  height: 79rpx;
  position: relative;
  left: 30rpx;
  top:8rpx;
