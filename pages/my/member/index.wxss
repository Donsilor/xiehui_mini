  box-shadow: 0 0 10rpx #e4e1df;
  margin:0 auto;
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
  padding: 40rpx 0 40rpx 0;
}

.main .content .one{
  width: 244rpx;
  height: 292rpx;
